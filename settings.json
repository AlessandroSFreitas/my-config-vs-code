{
  "workbench.colorTheme": "One Dark Pro",
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.tabSize": 4,
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "editor.rulers": [79],
    "editor.detectIndentation": true,
    "files.trimTrailingWhitespace": true,
    "tabnine.experimentalAutoImports": true,
    "files.insertFinalNewline": true,
    "breadcrumbs.enabled": true,
    "workbench.colorCustomizations": {
      "[One Dark Pro]": {
        // tab
        "tab.activeBackground": "#1B1726",
        "tab.inactiveBackground": "#1B1726",
        "tab.activeBorderTop":"#594D84",
        // activityBar
        "activityBar.activeBorder": "#594D84",
        "activityBarBadge.background": "#9e8ae9",
        "activityBar.background": "#1B1726",
        "activityBar.foreground": "#9e8ae9",
        // editor
        "editor.background": "#1B1726",
        "editorLineNumber.activeForeground": "#3C3354",
        "editorLineNumber.foreground": "#3C3354",
        "editorGroupHeader.tabsBackground": "#161221",
        "editor.lineHighlightBackground": "#241E34",
        // others
        "breadcrumb.foreground": "#3C3354",
        "progressBar.background": "#9e8ae9",
        "titleBar.activeBackground": "#13101D",
        "sideBar.background": "#13101D",
        "terminal.background": "#13101D",
        "statusBar.background": "#13101D",
        "sideBarSectionHeader.background": "#191622",
        "input.background": "#1B1726"
      },
    },
    "editor.tokenColorCustomizations": {
      "[One Dark Pro]": {
        "textMateRules": [
          {
            "scope": ["source.python"],
            "settings": {
              "foreground": "#e06c75"
            }
          }
        ]
      }
    },
    "workbench.startupEditor": "newUntitledFile",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.editorAssociations": [
      {
        "viewType": "jupyter.notebook.ipynb",
        "filenamePattern": "*.ipynb"
      }
    ],
    "python.showStartPage": false,
    "workbench.iconTheme": "material-icon-theme"
}

